<form>
    <div class="row">
        <div class="col-lg-4 col-lg-offset-3" style="margin-left:5% !important; margin-top: 2%">
            <div class="panel registration-form">
                <div class="panel-body">
                    <div class="text-center">
                        <div class="icon-object border-success text-success"><i class="icon-plus3"></i></div>
                        <h5 class="content-group-lg">Create account <small class="display-block">All fields are required</small></h5>
                    </div>

                    <div class="form-group has-feedback">
                        <input type="text" class="form-control" placeholder="Choose username" ng-model="username">
                        <div class="form-control-feedback">
                            <i class="icon-user-plus text-muted"></i>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group has-feedback">
                                <input type="text" class="form-control" placeholder="First name" ng-model="firstname">
                                <div class="form-control-feedback">
                                    <i class="icon-user-check text-muted"></i>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group has-feedback">
                                <input type="text" class="form-control" placeholder="Second name" ng-model="secondname">
                                <div class="form-control-feedback">
                                    <i class="icon-user-check text-muted"></i>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group has-feedback">
                                <input type="password" class="form-control" placeholder="Create password" ng-model="password" ng-change="change()">
                                <div class="form-control-feedback">
                                    <i class="icon-user-lock text-muted"></i>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group has-feedback">
                                <input type="password" class="form-control" placeholder="Repeat password" ng-model="confirmpassword" ng-change="change()">
                                <div class="form-control-feedback">
                                    <i class="icon-user-lock text-muted"></i>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group has-feedback">
                                <input type="email" class="form-control" placeholder="Your email" ng-model="email">
                                <div class="form-control-feedback">
                                    <i class="icon-mention text-muted"></i>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group has-feedback">
                                <input type="email" class="form-control" placeholder="Repeat email" ng-model="confirmemail">
                                <div class="form-control-feedback">
                                    <i class="icon-mention text-muted"></i>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="checkbox">
                            <label>
                                <input type="checkbox" class="styled" checked="checked">
                                Send me <a href="#">test account settings</a>
                            </label>
                        </div>

                        <div class="checkbox">
                            <label>
                                <input type="checkbox" class="styled" checked="checked">
                                Subscribe to monthly newsletter
                            </label>
                        </div>

                        <div class="checkbox">
                            <label>
                                <input type="checkbox" class="styled">
                                Accept <a href="#">terms of service</a>
                            </label>
                        </div>
                    </div>
                    <ul>
                        <li ng-repeat="e in errors" class="text-danger">{{e}}</li>
                    </ul>
                    <div class="text-right">
                        <a ui-sref="login.loginPartial" class="btn btn-link"><i class="icon-arrow-left13 position-left"></i> Back to login form</a>
                        <button type="submit" class="btn bg-teal-400 btn-labeled btn-labeled-right ml-10" ng-click="register()"><b><i class="icon-plus3"></i></b> Create account</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>